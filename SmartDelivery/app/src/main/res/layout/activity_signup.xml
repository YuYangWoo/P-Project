<?xml version="1.0" encoding="utf-8"?>

<layout xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">


        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <ImageView
                android:id="@+id/imageView2"
                android:layout_width="148dp"
                android:layout_height="55dp"
                app:srcCompat="@drawable/img_signup"
                tools:layout_editor_absoluteX="131dp"
                tools:layout_editor_absoluteY="53dp" />


            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">


                <EditText
                    android:id="@+id/ePhone"
                    android:layout_width="243dp"
                    android:layout_height="48dp"
                    android:backgroundTint="#1D78E5"
                    android:ems="10"
                    android:inputType="textPersonName"
                    android:singleLine="false"
                    tools:ignore="SpeakableTextPresentCheck"
                    tools:layout_editor_absoluteX="57dp"
                    tools:layout_editor_absoluteY="384dp" />

                <EditText
                    android:id="@+id/eId"
                    android:layout_width="243dp"
                    android:layout_height="48dp"
                    android:backgroundTint="#1D78E5"
                    android:ems="10"
                    android:inputType="textPersonName"
                    android:singleLine="false"
                    tools:ignore="SpeakableTextPresentCheck"
                    tools:layout_editor_absoluteX="56dp"
                    tools:layout_editor_absoluteY="147dp" />

                <EditText
                    android:id="@+id/eAddress"
                    android:layout_width="243dp"
                    android:layout_height="48dp"
                    android:backgroundTint="#1D78E5"
                    android:ems="10"
                    android:inputType="textPersonName"
                    android:singleLine="false"
                    tools:ignore="SpeakableTextPresentCheck"
                    tools:layout_editor_absoluteX="56dp"
                    tools:layout_editor_absoluteY="453dp" />

                <EditText
                    android:id="@+id/eName"
                    android:layout_width="243dp"
                    android:layout_height="48dp"
                    android:backgroundTint="#1D78E5"
                    android:ems="10"
                    android:inputType="textPersonName"
                    android:singleLine="false"
                    tools:ignore="SpeakableTextPresentCheck"
                    tools:layout_editor_absoluteX="57dp"
                    tools:layout_editor_absoluteY="302dp" />

                <EditText
                    android:id="@+id/eEmail"
                    android:layout_width="243dp"
                    android:layout_height="48dp"
                    android:backgroundTint="#1D78E5"
                    android:ems="10"
                    android:inputType="textPersonName"
                    android:singleLine="false"
                    tools:ignore="SpeakableTextPresentCheck"
                    tools:layout_editor_absoluteX="56dp"
                    tools:layout_editor_absoluteY="546dp" />

                <EditText
                    android:id="@+id/ePw"
                    android:layout_width="243dp"
                    android:layout_height="48dp"
                    android:backgroundTint="#1D78E5"
                    android:ems="10"
                    android:inputType="textPassword"
                    tools:ignore="SpeakableTextPresentCheck"
                    tools:layout_editor_absoluteX="56dp"
                    tools:layout_editor_absoluteY="221dp" />

                <ImageView
                    android:id="@+id/imageView4"
                    android:layout_width="16dp"
                    android:layout_height="16dp"
                    app:srcCompat="@drawable/img_id"
                    tools:layout_editor_absoluteX="58dp"
                    tools:layout_editor_absoluteY="130dp" />

                <ImageView
                    android:id="@+id/imageView6"
                    android:layout_width="45dp"
                    android:layout_height="16dp"
                    app:srcCompat="@drawable/img_name"
                    tools:layout_editor_absoluteX="58dp"
                    tools:layout_editor_absoluteY="286dp" />

                <ImageView
                    android:id="@+id/imageView7"
                    android:layout_width="53dp"
                    android:layout_height="16dp"
                    app:srcCompat="@drawable/img_phone"
                    tools:layout_editor_absoluteX="58dp"
                    tools:layout_editor_absoluteY="367dp" />

                <ImageView
                    android:id="@+id/imageView9"
                    android:layout_width="48dp"
                    android:layout_height="16dp"
                    app:srcCompat="@drawable/img_email"
                    tools:layout_editor_absoluteX="56dp"
                    tools:layout_editor_absoluteY="512dp" />

                <ImageView
                    android:id="@+id/imageView5"
                    android:layout_width="86dp"
                    android:layout_height="16dp"
                    app:srcCompat="@drawable/img_pw"
                    tools:layout_editor_absoluteX="56dp"
                    tools:layout_editor_absoluteY="205dp" />

                <ImageView
                    android:id="@+id/imageView8"
                    android:layout_width="71dp"
                    android:layout_height="16dp"
                    app:srcCompat="@drawable/img_address"
                    tools:layout_editor_absoluteX="57dp"
                    tools:layout_editor_absoluteY="437dp" />

                <android.widget.Button
                    android:id="@+id/sign_btn"
                    android:layout_width="0dp"
                    android:layout_height="50dp"
                    android:background="@color/mainColor"
                    android:text="@string/signup"
                    android:textColor="@color/white"
                    android:textSize="18sp"
                    tools:layout_editor_absoluteX="212dp"
                    tools:layout_editor_absoluteY="646dp" />


            </androidx.constraintlayout.widget.ConstraintLayout>

        </androidx.constraintlayout.widget.ConstraintLayout>
</layout>


